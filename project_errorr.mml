      \ AS way_pixels\n    FROM\n      (SELECT\n          ST_PointOnSurface(way) AS\
      \ way,\n          name,\n          access,\n          amenity,\n          barrier,\n\
      \          highway,\n          historic,\n          man_made,\n          railway,\n\
      \          tags,\n          way_area\n        FROM planet_osm_polygon\n    \
